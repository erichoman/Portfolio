{
  "scripts": {
    "watch": "concurrently \"npm run watch:tailwind\" \"npm run watch:flask\"",
    "watch:tailwind": "tailwindcss -i ./static/css/style.css -o ./static/css/output-style.css --watch",
    "watch:flask": "python app.py"
  },
  "dependencies": {
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "daisyui": "^4.12.23"
  }
}
